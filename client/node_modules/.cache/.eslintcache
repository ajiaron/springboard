nsole.log(err);\n        }\n      });\n      payButton.current.render(paypal.current);\n      setIsConfigured(false);\n    }\n  }, 0); // 500ms delay\n  useEffect(()=> {\n    debouncedUpdate(amount)\n    return () => {\n        debouncedUpdate.cancel()\n    }\n  }, [amount, name, charityid])\n\n  useEffect(() => {\n    if (isConfigured) {\n        prevAmount.current = amount;\n    }\n  }, [amount, isConfigured]);\n\n  return (\n    <div>\n        {(charityid && name && amount && amount > 0)?\n       <div ref={paypal} key={key} className={`${type}-button`}/>:\n       <div className=\"payment-confirm-container\">\n        <span className=\"payment-confirm-button\" onClick={()=>handleTest()}>\n            <p className={`${(amount && amount > 0)?'payment-confirm-text':'inactive-confirm-text '}`}>\n                Debug\n            </p>\n            {\n                (amount && amount > 0)?\n                <GoArrowRight className=\"arrow-icon\"/>:\n                null\n            }     \n        </span>\n        </div>\n    }\n    </div>\n  )\n}\n","/Users/aaronjiang/Desktop/reactjs/andale/client/src/components/Signin.js",["181","182","183","184","185","186"],[],"/Users/aaronjiang/Desktop/reactjs/andale/client/src/components/Login.js",["187","188","189","190"],[],"/Users/aaronjiang/Desktop/reactjs/andale/client/src/components/Register.js",["191","192"],[],"/Users/aaronjiang/Desktop/reactjs/andale/client/src/components/Profile.js",["193","194","195","196","197","198"],[],{"ruleId":"199","severity":1,"message":"200","line":1,"column":16,"nodeType":"201","messageId":"202","endLine":1,"endColumn":24},{"ruleId":"199","severity":1,"message":"203","line":1,"column":26,"nodeType":"201","messageId":"202","endLine":1,"endColumn":35},{"ruleId":"199","severity":1,"message":"200","line":1,"column":16,"nodeType":"201","messageId":"202","endLine":1,"endColumn":24},{"ruleId":"199","severity":1,"message":"204","line":1,"column":26,"nodeType":"201","messageId":"202","endLine":1,"endC